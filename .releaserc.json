{
    "branches": ["main", "mnordby/test"],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/exec",
            {
                "print": "echo LS",
                "print2": "ls",
                "print3": "echo ${nextRelease.version}",
                "generateNotesCmd": "python update_version.py ${nextRelease.version}"
            }
        ],
        "@semantic-release/changelog",
        [
            "@semantic-release/git",
            {
                "assets": ["README.md", "*.json"],
                "message": "chore(release): update version to ${nextRelease.version} [skip ci]"
            }
        ],
        "@semantic-release/github"
    ]
}
