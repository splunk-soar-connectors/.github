{
    "branches": ["main", "mnordby/test"],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/changelog",
        [
            "@semantic-release/exec",
            {
                "updateVersionInFiles": "python .github/actions/publish/utils/update_version.py ${nextRelease.version}"
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": ["README.md", "*.json"],
                "message": "chore(release): update version to ${nextRelease.version} [skip ci]"
            }
        ],
        "@semantic-release/github"
    ]
}
